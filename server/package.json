{
    "name": "my-doc-processor",
    "version": "1.0.0",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpileOnly src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "watch": "tsc --watch",
        "build-run": "npm run build && npm run start",
        "watch-run": "nodemon --watch 'src/**/*.ts' --exec 'npm run build-run'"
    },
    "dependencies": {
        "axios": "^1.3.2",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "fs-extra": "^11.3.0",
        "multer": "^1.4.5-lts.1",
        "nodemon": "^3.1.9",
        "openai": "^3.2.1",
        "sharp": "^0.33.5",
        "tesseract.js": "^4.0.2",
        "uuid": "^11.0.5",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.15",
        "@types/fs-extra": "^11.0.4",
        "@types/multer": "^1.4.12",
        "@types/node": "^18.11.18",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.5"
    }
}
